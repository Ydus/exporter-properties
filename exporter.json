{
    "id": "io.supernova.documentation-main",
    "name": "Documentation (test)",
    "description": "Test Supernova documentation exporter",
    "author": "Jiri Trecak",
    "organization": "Supernova",
    "source_dir": "src",
    "assets_dir": "assets",
    "version": "1.2.7",
    "config": {
        "sources": "sources.json",
        "output": "output.json",
        "js": "src/js_helpers.js"
    },
    "engines": {
        "pulsar": "1.0.0",
        "supernova": "1.0.0"
    },
    "type": "documentation",
    "tags": [
        "Documentation"
    ],
    "contributes": {
        "configuration": [
            {
                "key": "custom_string",
                "type": "string",
                "category": "Custom properties",
                "label": "Custom string",
                "description": "Le custom string",
                "default": "Hue hue hue"
            },
            {
                "key": "custom_number",
                "type": "number",
                "category": "Custom properties",
                "label": "Custom number",
                "description": "Le custom number",
                "default": 69
            },
            {
                "key": "custom_boolean",
                "type": "boolean",
                "category": "Custom properties",
                "label": "Custom boolean",
                "description": "Le custom boolean",
                "default": true
            },
            {
                "key": "custom_enum",
                "type": "enum",
                "category": "Custom properties",
                "label": "Custom enum",
                "description": "Le custom enum",
                "values": ["First", "Second", "Third"],
                "default": "Second"
            },
            {
                "key": "custom_image",
                "type": "image",
                "category": "Custom properties",
                "label": "Custom image",
                "description": "Le custom image"
            },
            {
                "key": "custom_string_2",
                "type": "string",
                "category": "Custom properties 2",
                "label": "Custom string 2",
                "description": "Le custom string 2",
                "default": "Hue hue hue"
            }
        ]
    }
}
